{

    "manifest_version": 2,
    "name": "Extension",
    "version": "0.1.0",
  
    "description": "Adds a green border to all webpages matching hariomgaur.in",
  
    "icons": {
      "48": "icons/extension-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.hariomgaur.in/*"],
        "js": ["extension.js"]
      }
    ]
  
  }
  